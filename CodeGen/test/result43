int main() {
int i =1;
int j;
for (i = 1; i <= 5; i=i+1) {
  putIntLn(i);
  if (i == 3) continue;
  j = 1;
  while (j <= 3) {  
    putIntLn(j);
    if (j == 3) break;
    j = j + 1;
    }
  putLn();
  }
return 0;
}  

